{
    "status": "ON",
    "serviceType": "ps:tools:blipp",
    "name": "blipp",
    "ttl": 100000,
    "location":{"institution": "GEMINI"},
    "description": "BLiPP running on a GENI slice",
    "properties": {
	"configurations": {
            "unis_poll_interval": 30,
	    "unis_url": "https://unis.incntre.iu.edu:8443",
	    "use_ssl": true,
            "ssl_cert": "/usr/local/etc/certs/mp_cert.pem",
            "ssl_key": "/usr/local/etc/certs/mp_key.pem",
	    "ssl_cafile": "",
	    "probe_defaults":
	    {"collection_schedule":"builtins.simple",
	     "schedule_params": {"every": 2},
	     "reporting_params": 8,
	     "collection_size":10000000,
	     "collection_ttl":1500000
	    },
	    "probes":{
		"cpu":{
		    "probe_module": "cpu",
		    "kwargs":{"proc_dir":"/proc"}
		},
		"net":{
		    "probe_module": "net",
		    "kwargs":{"unis_url": "https://unis.incntre.iu.edu:8443"}
		},
		"mem": {
		    "probe_module": "mem",
		    "kwargs":{}
		}
	    }
	},
	"summary": {
	    "metadata": []
	}
    }
}